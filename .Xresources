URxvt*termName:		rxvt-unicode-256color
URxvt.scrollBar:	off
URxvt.internalBorder:	0

Xtf.dpi:                96
Xtf.antialias:          true
Xtf.hinting:            true
Xtf.rgba:               rgb
Xtf.autohint:           false
Xtf.hintstyle:          hintfull
Xtf.lcdfilter:          lcddefault

URxvt*font:		xft:JetBrains Mono:size=14:antialias=true
URxvt*boldFont:		xft:JetBrains Mono:size=14:antialias=true
URxvt*italicFont:	xft:JetBrains Mono:size=14:antialias=true
URxvt*boldItalicFont:	xft:JetBrains Mono:size=14:antialias=true
URxvt*letterSpace:	-1
URxvt.cursorUnderline:  true
URxvt.iso14755:         false

